<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>
	
    <definition name="protocol/protocolform/protocol-form-template" template="/WEB-INF/layouts/protocol/protocolform/protocol-form-template.jspx">
		<put-list-attribute name="styles">
			
			<add-attribute value="/static/styles/application.css" type="string" />
			<add-attribute value="/static/styles/forms.css" type="string" />
			<add-attribute value="/static/styles/icons.css" type="string" />
		</put-list-attribute>
		<put-list-attribute name="ie_styles">
			<add-attribute value="/static/styles/application_ie.css" type="string" />
		</put-list-attribute>
		<put-list-attribute name="javascripts"><add-attribute value="/static/js/encoder.js" /><add-attribute value="/static/js/moment.js" />
		</put-list-attribute>
	</definition>
	
	<definition name="protocol/protocolform/protocol-form-template-noheader" template="/WEB-INF/layouts/protocol/protocolform/protocol-form-template-noheader.jspx">
		<put-list-attribute name="styles">
			
			<add-attribute value="/static/styles/application.css" type="string" />
			<add-attribute value="/static/styles/forms.css" type="string" />
			<add-attribute value="/static/styles/forms-no-header.css" type="string" />
			<add-attribute value="/static/styles/icons.css" type="string" />
		</put-list-attribute>
		<put-list-attribute name="ie_styles">
			<add-attribute value="/static/styles/application_ie.css" type="string" />
		</put-list-attribute>
		<put-list-attribute name="javascripts"><add-attribute value="/static/js/encoder.js" /><add-attribute value="/static/js/moment.js" />
		</put-list-attribute>
	</definition>

</tiles-definitions>
