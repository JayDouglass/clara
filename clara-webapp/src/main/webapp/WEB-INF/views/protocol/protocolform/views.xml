<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>

	 <definition extends="default" name="protocol/protocolform/dashboard">
        <put-attribute name="body" value="/WEB-INF/views/protocol/protocolform/dashboard.jspx"/>
        <put-list-attribute name="javascripts" inherit="true">
			<add-attribute value="/static/js/protocol/protocolform/dashboard.js" />
  		</put-list-attribute>
  		<put-list-attribute name="styles" inherit="true">
        	<add-attribute value="/static/styles/protocol/protocolform/dashboard.css" />
  		</put-list-attribute>
    </definition>    

    <definition extends="default-noheader" name="protocol/protocolform/protocolFormLockedException">
		<put-attribute name="body" value="/WEB-INF/views/protocol/protocolform/protocolFormLockedException.jspx"/>
		<put-list-attribute name="javascripts" inherit="true">
    		<add-attribute value="/static/js/protocol/protocolform/protocolform-locked.js" />
  		</put-list-attribute>
	</definition>	
	
</tiles-definitions>
