<div class="clara-content-area"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:form="http://www.springframework.org/tags/form"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:roo="urn:jsptagdir:/WEB-INF/tags"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:x="http://java.sun.com/jsp/jstl/xml"
	xmlns:p="http://devsphere.com/xml/taglib/process"
	xmlns:uams="urn:jsptagdir:/WEB-INF/tags" version="2.0">

	<jsp:output omit-xml-declaration="yes" />

	<c:import url="/static/xml/lookup.xml" var="lookupxml" />

	<c:import var="helpxml" url="/static/xml/help.xml" />

	<x:parse doc="${protocolFormXmlData.xmlData}" var="protocolInstance" />
	<x:set var="protocolTitle"
		select="string($protocolInstance/protocol/title)" />
	<x:set var="studyType"
		select="string($protocolInstance/protocol/study-nature/text())" />
	<x:parse doc="${lookupxml}" var="lookup" scope="application" />
	<x:parse doc="${helpxml}" var="help" scope="application" />

	<x:out escapeXml="false" select="string($protocolInstance/text())" />

	<div style="display:none;" id="claraFormTitle"><c:out value="${fn:escapeXml(fn:trim(fn:replace(protocolTitle, '\\n',' ')))}"/></div><div id="clara-form-pagelist">
		<uams:newsubmissiontab name="subjects"
			protocolInstance="${protocolInstance}" />
	</div>

	<div id="clara-form-pagecontent">


		<div class="formArea">

			<input id="pagefragment" name="pagefragment" type="hidden" value="" />

			<div id='page-wizard-wrapper'>
				<!--  //  -->
			</div>

			<div class="question" id="question-total-number-subjects">
				<div class="questionIdentifier">Subjects-1</div>
				<div class="questionLabel">
					<span class="question-label-text">Provide the total number
						of subjects (or number of participant records, specimens, etc.)
						for whom you are seeking UAMS IRB oversight.</span>
				</div>
				<div class="questionValue">
					<x:set var="totalNumber"
						select="string($protocolInstance/protocol/subjects/total-number/text())" />
					<uams:textbox id="total-number"
						instancepath="/protocol/subjects/total-number"
						validation="number required" value="${totalNumber}" />
				</div>
				<div style="clear: both;">
					<!-- // -->
				</div>
				<div class="questionHelp">
					<uams:helpinfo lookupid="total-number-subjects" />
				</div>
			</div>

			<div class="question" id="question-total-accural-goal-for-all-sites">
				<div class="questionIdentifier">Subjects-2</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since this is a multi-site study, please answer the following
						question:</h1>
					<span class="question-label-text">What is the total subject
						accrual goal for all sites for this study (if available)?</span>
				</div>
				<div class="questionValue">
					<x:set var="totalAccuralGoalForAllSites"
						select="string($protocolInstance/protocol/subjects/multiple-sites/y/total-accrual-goal-for-all-sites/text())" />
					<uams:textbox id="total-accural-goal-for-all-sites"
						instancepath="/protocol/subjects/multiple-sites/y/total-accrual-goal-for-all-sites"
						value="${totalAccuralGoalForAllSites}" />
				</div>
			</div>
			
			<div class="question clearfix"
				id="question-age-ranges">
				<div class="questionIdentifier">Subjects-3</div>
				<div class="questionLabel">
					<span class="question-label-text">What age range(s) do the subjects fall into?</span>
				</div>
				<div class="questionValue">
					<x:set var="ageRanges"
						select="$protocolInstance/protocol/subjects/age-ranges/age-range" />
					<uams:checkbox id="age-ranges"
						value="${ageRanges}" newColumnEvery="5"
						lookuppath="age-ranges"
						instancepath="/protocol/subjects/age-ranges/age-range"
						validation="required" />
				</div>
				<div style="clear: both;">
					<!-- // -->
				</div>
				<div class="questionHelp">
					<uams:helpinfo lookupid="age-ranges" />
				</div>
			</div>
			

			<div class="question" id="question-subject-population">
				<div class="questionIdentifier">Subjects-4</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since this is a social
						behavioral education related study, please answer the following
						question:</h1>
					<span class="question-label-text">Name and describe the
						subject population(s) for the study. </span>
				</div>
				<div class="questionValue">
					<input type="hidden"
						class="question-el dummy-field-for-rule-activation"
						name="/protocol/study-subject-populations/population" />
					<div id="multiple-population-list">
						<!-- // -->
					</div>
				</div>
				<div class="questionHelp">
					<uams:helpinfo lookupid="target-multiple-populations" />
				</div>
			</div>
			
			<div class="question clearfix"
				id="question-vulnerable-populations-included">
				<div class="questionIdentifier">Subjects-5</div>
				<div class="questionLabel">
					<span class="question-label-text"> Select all of the
						vulnerable populations that will be, or potentially could be,
						included in the study:</span>
				</div>
				<div class="questionValue">
					<x:set var="vulnerablepopulationsIncluded"
						select="$protocolInstance/protocol/subjects/vulnerable-populations/included" />
					
					<uams:checkbox id="vulnerable-populations-included"
						value="${vulnerablepopulationsIncluded}" newColumnEvery="5"
						allowcustom="true" lookuppath="vulnerable-populations"
						instancepath="/protocol/subjects/vulnerable-populations/included"
						validation="required" />
				</div>
				<div style="clear: both;">
					<!-- // -->
				</div>
				<div class="questionHelp">
					<uams:helpinfo lookupid="vulnerable-populations" />
				</div>
			</div>

			<div class="question clearfix"
				id="question-vulnerable-populations-targeted">
				<div class="questionIdentifier">Subjects-6</div>
				<div class="questionLabel">
					<span class="question-label-text">Select all of the
						vulnerable populations that will be targeted for participation in
						the study: </span>
				</div>
				<div class="questionValue">
					<x:set var="vulnerablepopulationsTargeted"
						select="$protocolInstance/protocol/subjects/vulnerable-populations/targeted" />
					<uams:checkbox id="vulnerable-populations-targeted"
						value="${vulnerablepopulationsTargeted}" newColumnEvery="5"
						allowcustom="true" lookuppath="vulnerable-populations-targeted"
						instancepath="/protocol/subjects/vulnerable-populations/targeted"
						validation="required" />
				</div>
				<div style="clear: both;">
					<!-- // -->
				</div>
				<div class="questionHelp">
					<uams:helpinfo lookupid="vulnerable-populations-targeted" />
				</div>
			</div>

			<!-- APPENDIX A Pregnant Women, Fetuses -->
			<div class="question clearfix"
				data-subsection-title="Pregnant Women, Fetuses"
				id="question-pregnant-women-studies-for-assessing-risks">
				<div class="questionIdentifier">Subjects-Appx.A-1</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Pregnant
						Women/Fetuses will be potentially included in this study, answer the
						following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">Describe preclinical
						studies (including studies on pregnant animals) and clinical
						studies (including studies on non-pregnant women), that provide data for assessing
						potential risks to pregnant women and fetuses.</span>
				</div>
				<div class="questionValue">
					<x:set var="studiesForAssessingRisks" select="string($protocolInstance/protocol/subjects/vulnerable-populations/pregnant-wormen-or-fetuses/studies-for-assessing-risks/text())" />
					<uams:textarea validation="required"
						instancepath="/protocol/subjects/vulnerable-populations/pregnant-wormen-or-fetuses/studies-for-assessing-risks"
						id="women-studies-for-assessing-risks" value="${studiesForAssessingRisks}" hasNA="true" />
				</div>
				<div class="questionHelp">
					<uams:helpinfo
						lookupid="pregnant-women-studies-for-assessing-risks" />
				</div>
			</div>

			<div class="question clearfix"
				data-subsection-title="Pregnant Women, Fetuses"
				id="question-pregnant-women-direct-benefit">
				<div class="questionIdentifier">Subjects-Appx.A-2</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Pregnant
						Women/Fetuses will be potentially included in this study, answer the
						following question:</h1>
				</div>
				<div class="questionLabel">
					Indicate for whom the prospect of <b>direct benefit</b> exists:
				</div>
				<div class="questionValue">
					<x:set var="directBenefit"
						select="string($protocolInstance/protocol/subjects/vulnerable-populations/pregnant-wormen-or-fetuses/direct-benefit-exist-for-whom/text())" />
					<uams:radio id="pregnant-women-direct-benefit"
						value="${directBenefit}" lookuppath="subject-direct-benefit"
						instancepath="/protocol/subjects/vulnerable-populations/pregnant-wormen-or-fetuses/direct-benefit-exist-for-whom"
						validation="required" />
				</div>
				<div class="questionHelp">
					<ul class="questionHelp-list">
						<li>The father's consent must be obtained for research that
							holds the prospect of direct benefit solely to the fetus, unless
							he is unable to consent because of unavailability, incompetence,
							or temporary incapacity or the pregnancy resulted from rape or
							incest.</li>
					</ul>
				</div>
			</div>

			<div class="question clearfix"
				data-subsection-title="Pregnant Women, Fetuses"
				id="question-fetus-risk">
				<div class="questionIdentifier">Subjects-Appx.A-3</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Pregnant
						Women/Fetuses will be potentially included in this study, answer the
						following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">The risk to the fetus is
						(check one):</span>
				</div>
				<div class="questionValue">
					<x:set var="riskFetus"
						select="string($protocolInstance/protocol/subjects/vulnerable-populations/pregnant-wormen-or-fetuses/risk-to-fetus/text())" />
					<uams:radio id="fetus-risk" value="${riskFetus}"
						lookuppath="subject-risk-fetus"
						instancepath="/protocol/subjects/vulnerable-populations/pregnant-wormen-or-fetuses/risk-to-fetus"
						validation="required" />
				</div>
			</div>

			<div class="question clearfix"
				data-subsection-title="Pregnant Women, Fetuses"
				id="question-pregnant-wormen-least-possibel-for-achiveving">
				<div class="questionIdentifier">Subjects-Appx.A-4</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Pregnant
						Women/Fetuses will be potentially included in this study, answer the
						following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">Explain how the risks are
						the least possible for achieving the objectives of the research.</span>
				</div>
				<div class="questionValue">
					<x:set var="leastPossibleForAchieving" select="string($protocolInstance/protocol/subjects/vulnerable-populations/pregnant-wormen-or-fetuses/least-possible-for-achieving/text())" />
					<uams:textarea validation="required"
						instancepath="/protocol/subjects/vulnerable-populations/pregnant-wormen-or-fetuses/least-possible-for-achieving"
						id="pregnant-wormen-least-possible-for-achiveving" value="${leastPossibleForAchieving}" hasNA="false" />
				</div>
			</div>

			<!-- APPENDIX B (Neonates)-->
			<div class="question clearfix" data-subsection-title="Neonates"
				id="question-neonates-determine-person">
				<div class="questionIdentifier">Subjects-Appx.B-1</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Neonates will be
						potentially included in this study, answer the following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">State who (other than
						investigators and key personnel) will determine the viability of a
						neonate and describe what procedures will be used to determine
						viability.</span>
				</div>
				<div class="questionValue">
					<x:set var="determinePerson" select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/determine-person/text())" />
					<uams:textarea validation="required"
						instancepath="/protocol/subjects/vulnerable-populations/neonates/determine-person"
						id="neonates-determine-person" value="${determinePerson}" hasNA="false" />
				</div>
			</div>

			<div class="question clearfix" data-subsection-title="Neonates"
				id="question-neonates-studies-for-assessing-risks">
				<div class="questionIdentifier">Subjects-Appx.B-2</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Neonates will be
						potentially included in this study, answer the following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">Describe preclinical
						studies and clinical studies, where scientifically appropriate,
						that provide data for assessing potential risks to neonates.</span>
				</div>
				<div class="questionValue">
					<x:set var="studiesForAssessingRisks" select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/studies-for-assessing-risks/text())" />
					<uams:textarea validation="required"
						instancepath="/protocol/subjects/vulnerable-populations/neonates/studies-for-assessing-risks"
						id="neonates-studies-for-assessing-risks" value="${studiesForAssessingRisks}" hasNA="false" />
				</div>
			</div>

			<div class="question clearfix" data-subsection-title="Neonates"
				id="question-neonates-viability">
				<div class="questionIdentifier">Subjects-Appx.B-3</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Neonates will be
						potentially included in this study, answer the following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">The viability of neonates
						to be involved in the research is (Check one):</span>
				</div>
				<div class="questionValue">
					<x:set var="viabilityNeonates"
						select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/viability/text())" />
					<uams:radio id="neonates-viability"
						lookuppath="subject-viability-neonates"
						instancepath="/protocol/subjects/vulnerable-populations/neonates/viability"
						value="${viabilityNeonates}" validation="required" />
				</div>
			</div>

			<div class="question clearfix" data-subsection-title="Neonates"
				id="question-neonates-uncertain-viability-risk">
				<div class="questionIdentifier">Subjects-Appx.B-3.1.1</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since neonates of
						uncertain viability will be potentially included in this study,
						answer the following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">For neonates of uncertain
						viability, the risk to the neonate is (check one):</span>
				</div>
				<div class="questionValue">
					<x:set var="uncertainViabilityNeonatesRisk"
						select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/viability/uncertain-viability/risk-to-neonate/text())" />
					<uams:radio id="neonates-uncertain-viability-risk"
						value="${uncertainViabilityNeonatesRisk}"
						lookuppath="subject-neonates-uncertain-viability-risk"
						instancepath="/protocol/subjects/vulnerable-populations/neonates/viability/uncertain-viability/risk-to-neonate"
						validation="required" />
				</div>
				<br /> <br />
				<div class="questionLabel">
					<span class="question-label-text">Explain how the research
						meets the condition selected above:</span>
				</div>
				<div class="questionValue">
					<x:set var="howResearchMeetRiskConditions" select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/viability/uncertain-viability/how-research-meet-risk-conditions/text())" />
					<uams:textarea instancepath="/protocol/subjects/vulnerable-populations/neonates/viability/uncertain-viability/how-research-meet-risk-conditions"
						value="${howResearchMeetRiskConditions}" hasNA="true" id="how-research-meet-risk-conditions" />
				</div>
				<div class="questionHelp">
					<uams:helpinfo lookupid="neonates-uncertain-viability-risk" />
				</div>
			</div>

			<div class="question clearfix" data-subsection-title="Neonates"
				id="question-neonates-nonviable-research-inclusion">
				<div class="questionIdentifier">Subjects-Appx.B-3.2.1</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since "nonviable
						neonates" will be potentially included in this study, please
						answer the following question:</h1>
					<span class="question-label-text">For nonviable neonates,
						explain how EACH of the following 4 conditions for inclusion is
						met:</span><br /> 1. Vital functions of the neonate will not be
					artificially maintained.
				</div>
				<div class="questionValue">
					<x:set var="vitalFunctions" select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/viability/nonviable/how-research-meet-inclusion-conditions/vital-functions/text())" />
					<uams:textarea instancepath="/protocol/subjects/vulnerable-populations/neonates/viability/nonviable/how-research-meet-inclusion-conditions/vital-functions"
						id="vital-functions" value="${vitalFunctions}" hasNA="false"/>
				</div>
				<br />
				<div class="questionLabel">2. The research will not terminate
					the heartbeat or respiration of the neonate.</div>
				<div class="questionValue">
					<x:set var="terminateHeartbeat" select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/viability/nonviable/how-research-meet-inclusion-conditions/terminate-heartbeat/text())" />
					<uams:textarea instancepath="/protocol/subjects/vulnerable-populations/neonates/viability/nonviable/how-research-meet-inclusion-conditions/terminate-heartbeat"
						id="terminate-heartbeat" value="${terminateHeartbeat}" hasNA="false"/>
				</div>
				<br />
				<div class="questionLabel">3. There will be no added risk to
					the neonate resulting from the research; and</div>
				<div class="questionValue">
					<x:set var="noAddedRisk" select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/viability/nonviable/how-research-meet-inclusion-conditions/no-added-risk/text())" />
					<uams:textarea instancepath="/protocol/subjects/vulnerable-populations/neonates/viability/nonviable/how-research-meet-inclusion-conditions/no-added-risk"
						id="no-added-risk" value="${noAddedRisk}" hasNA="false"/>
				</div>
				<br />
				<div class="questionLabel">4. The purpose of the research is
					the development of important knowledge that cannot be obtained by
					other means.</div>
				<div class="questionValue">
					<x:set var="developmentOfImportantKnowledge" select="string($protocolInstance/protocol/subjects/vulnerable-populations/neonates/viability/nonviable/how-research-meet-inclusion-conditions/development-of-important-knowledge/text())" />
					<uams:textarea instancepath="/protocol/subjects/vulnerable-populations/neonates/viability/nonviable/how-research-meet-inclusion-conditions/development-of-important-knowledge"
						id="development-of-important-knowledge" value="${developmentOfImportantKnowledge}" hasNA="false"/>
				</div>
				<div class="questionHelp">
					<uams:helpinfo lookupid="neonates-nonviable-research-inclusion" />
				</div>
			</div>



			<!-- APPENDIX  D (Prisoners)-->
			<div class="question clearfix" data-subsection-title="Prisoners"
				id="question-prisoners-involvement-categories">
				<div class="questionIdentifier">Subjects-Appx.D-1</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Prisoners will be
						potentially included in this study, answer the following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">Indicate the category(ies)
						that best describes the involvement of prisoners in the research:</span>
				</div>
				<div class="questionValue">
					<x:set var="involvementPrisoners"
						select="$protocolInstance/protocol/subjects/vulnerable-populations/prisoners/involvement-of-prisoners-categories" />
					<uams:checkbox value="${involvementPrisoners}"
						id="prisoners-involvement-categories"
						lookuppath="subject-involvement-prisoners"
						instancepath="/protocol/subjects/vulnerable-populations/prisoners/involvement-of-prisoners-categories"
						validation="required" />
				</div>
			</div>

			<div class="question clearfix" data-subsection-title="Prisoners"
				id="question-prisoners-describe-facility">
				<div class="questionIdentifier">Subjects-Appx.D-2</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Prisoners will be
						potentially included in this study, answer the following question:</h1>
				</div>
				<div class="questionLabel">
					<span class="question-label-text">Provide the name, type of
						facility and location of each local, state, or federal facility to
						be used.</span>
				</div>
				<div class="questionValue">
					<x:set var="describeFacility" select="string($protocolInstance/protocol/subjects/vulnerable-populations/prisoners/describe-facility/text())" />
					<uams:textarea validation="required" instancepath="/protocol/subjects/vulnerable-populations/prisoners/describe-facility"
						id="describe-facility" value="${describeFacility}" hasNA="false"/>
				</div>
			</div>


			<div class="question" data-subsection-title="Prisoners"
				id="question-prisoners-advantages-to-participating-prisoners">
				<div class="questionIdentifier">Subjects-Appx.D-3</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Prisoners will be
						potentially included in this study, answer the following question:</h1>
					<span class="question-label-text">Describe the possible
						advantages to participating prisoners (i.e., compared to the
						general living conditions, medical care, quality of food,
						amenities, and opportunity for earnings in the prison).</span>
				</div>
				<div class="questionValue">
					<x:set var="advantagesToParticipatingPrisoners" select="string($protocolInstance/protocol/subjects/vulnerable-populations/prisoners/advantages-to-participating-prisoners/text())" />
					<uams:textarea validation="required" instancepath="/protocol/subjects/vulnerable-populations/prisoners/advantages-to-participating-prisoners"
						id="advantages-to-participating-prisoners" value="${advantagesToParticipatingPrisoners}" hasNA="true"/>
				</div>
			</div>

			<div class="question" data-subsection-title="Prisoners"
				id="question-prisoners-plans-to-avoid-undue-influence">
				<div class="questionIdentifier">Subjects-Appx.D-4</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since Prisoners will be
						potentially included in this study, answer the following question:</h1>
					<span class="question-label-text">Describe any additional
						steps that will be taken to avoid undue influence (i.e.,
						considering the limited choice environment of the prison).</span>
				</div>
				<div class="questionValue">
					<x:set var="plansToAvoidUndueInfluence" select="string($protocolInstance/protocol/subjects/vulnerable-populations/prisoners/plans-to-avoid-undue-influence/text())" />
					<uams:textarea validation="required" instancepath="/protocol/subjects/vulnerable-populations/prisoners/plans-to-avoid-undue-influence"
