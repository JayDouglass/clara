					<uams:textarea validation="required" instancepath="/protocol/subjects/compensation-provided-to-subjects/y/method-and-amount-of-compensation"
						id="method-and-amount-of-compensation" value="${methodAndAmountOfCompensation}" hasNA="false"/>
				</div>
