					<x:set var="bloodAmount" select="string($protocolInstance/protocol/risks/storage-of-biological-materials/will-specimens-be-obtained/y/will-specimens-be-collected/y/types-of-specimens/blood/amount/text())" />
					<uams:textarea validation="required" instancepath="/protocol/risks/storage-of-biological-materials/will-specimens-be-obtained/y/will-specimens-be-collected/y/types-of-specimens/blood/amount"
