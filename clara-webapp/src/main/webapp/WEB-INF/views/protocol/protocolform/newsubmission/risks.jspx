					<uams:textarea validation="required" instancepath="/protocol/risks/storage-of-biological-materials/will-specimens-be-obtained/y/will-specimens-be-collected/y/types-of-specimens/blood/locations"
						id="blood-locations" value="${bloodLocations}" hasNA="false"/>
				</div>
				<br />
				<br />
				<div class="questionLabel">
					<div class="questionIdentifier">Risks-4.1.1.1.1.1.4</div>
