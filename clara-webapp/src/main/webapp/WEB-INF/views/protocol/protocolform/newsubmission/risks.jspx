					<uams:textarea validation="required" instancepath="/protocol/risks/storage-of-biological-materials/will-specimens-be-obtained/y/will-specimens-be-collected/y/types-of-specimens/blood/resources-for-safety"
						id="resources-for-safety" value="${resourcesForSafety}" hasNA="false"/>
