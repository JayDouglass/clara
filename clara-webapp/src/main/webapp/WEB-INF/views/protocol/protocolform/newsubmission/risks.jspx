<div class="clara-content-area"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:form="http://www.springframework.org/tags/form"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:roo="urn:jsptagdir:/WEB-INF/tags"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:x="http://java.sun.com/jsp/jstl/xml"
	xmlns:p="http://devsphere.com/xml/taglib/process"
	xmlns:uams="urn:jsptagdir:/WEB-INF/tags" version="2.0">

	<jsp:output omit-xml-declaration="yes" />

	<c:import var="helpxml" url="/static/xml/help.xml" />
	<c:import url="/static/xml/lookup.xml" var="lookupxml" />

	<x:parse doc="${protocolFormXmlData.xmlData}" var="protocolInstance" />
	<x:parse doc="${lookupxml}" var="lookup" scope="application" />
	<x:parse doc="${helpxml}" var="help" scope="application" />

	<x:set var="protocolTitle"
		select="string($protocolInstance/protocol/title)" />


	<div style="display:none;" id="claraFormTitle"><c:out value="${fn:escapeXml(fn:trim(fn:replace(protocolTitle, '\\n',' ')))}"/></div><div id="clara-form-pagelist">
		<uams:newsubmissiontab name="risks"
			protocolInstance="${protocolInstance}" />
	</div>

	<div id="clara-form-pagecontent">

		<div class="formArea">
			<input id="pagefragment" name="pagefragment" type="hidden" value="" />

			<div id='page-wizard-wrapper'>
				<!--  //  -->
			</div>


			<div class="question"
				id="question-resource-to-protect-human-study-participants"
				data-subsection-title="Study Procedure">
				<div class="questionLabel">
					<h1 class="subsection-question-label">Study Procedure</h1>
					<div class="questionIdentifier">Risks-1</div>
					<span class="question-label-text">List the resources
						available to protect human study participants in the research
						settings.</span>
				</div>
				<div class="questionValue">
					<x:set var="resourceToProtectHumanStudyParticipants" select="string($protocolInstance/protocol/risks/storage-of-biological-materials/resource-to-protect-human-study-participants/text())" />
					<uams:textarea validation="required" instancepath="/protocol/risks/storage-of-biological-materials/resource-to-protect-human-study-participants"
						id="resource-to-protect-human-study-participants" value="${resourceToProtectHumanStudyParticipants}" hasNA="false"/>
				</div>
				<div style="clear: both;">
					<!-- // -->
				</div>
				<div class="questionHelp">
					<uams:helpinfo
						lookupid="resource-to-protect-human-study-participants" />
				</div>
			</div>

			
			<div class="question"
				id="question-research-records"
				data-subsection-title="Study Procedure">
				<div class="questionIdentifier">Risks-2</div>
				<div class="questionLabel">
					<span class="question-label-text">Indicate whether the research records will be:</span>
				</div>
				<div class="questionValue">
					<x:set var="researchRecords"
						select="string($protocolInstance/protocol/risks/storage-of-biological-materials/research-records/text())" />
					<uams:radio value="${researchRecords}"
						lookuppath="specimens-identification"
						instancepath="/protocol/risks/storage-of-biological-materials/research-records"
						validation="required" />
				</div>
				<div style="clear: both;">
					<!-- // -->
				</div>
				<div class="questionHelp">
					<uams:helpinfo
						lookupid="specimens-be-obtained-specimen-identification" />
				</div>
			</div>
			
			<div class="question"
				id="question-research-records-process"
				data-subsection-title="Study Procedure">
				<div class="questionIdentifier">Risks-2.1.1</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since research records will
						be coded or de-identified, answer the following question:</h1>
					<span class="question-label-text">Describe the process used
						to code or de-identify the research records.</span>
				</div>
				<div class="questionValue">
					<x:set var="researchRecordsProcess" select="string($protocolInstance/protocol/risks/storage-of-biological-materials/research-records/coded-or-deidentified/process/text())" />
					<uams:textarea validation="required" instancepath="/protocol/risks/storage-of-biological-materials/research-records/coded-or-deidentified/process"
						id="research-records-process" value="${researchRecordsProcess}"/>
				</div>
			</div>
			
			<div class="question"
				id="question-explain-research-records-process"
				data-subsection-title="Study Procedure">
				<div class="questionIdentifier">Risks-2.2.1</div>
				<div class="questionLabel">
					<h1 class="conditional-question-label">Since research records will
						be neither coded nor de-identified, answer the following question:</h1>
					<span class="question-label-text">Please explain.</span>
				</div>
				<div class="questionValue">
					<x:set var="explainResearchRecordsProcess" select="string($protocolInstance/protocol/risks/storage-of-biological-materials/research-records/neither/explain/text())" />
					<uams:textarea validation="required" instancepath="/protocol/risks/storage-of-biological-materials/research-records/neither/explain"
						id="explain-research-records-process" value="${explainResearchRecordsProcess}"/>
				</div>
			</div>
			
			<div class="question"
				id="question-desc-procedures-for-data-storage"
				data-subsection-title="Study Procedure">
				<div class="questionIdentifier">Risks-3</div>
				<div class="questionLabel">
					<span class="question-label-text">Describe the procedures for record storage.  Where will electronic and/or hard copy 
						research records be stored?  Who will have access to them?  When and how will they be destroyed?</span>
				</div>
				<div class="questionValue">
					<x:set var="descProceduresForDataStorage" select="string($protocolInstance/protocol/risks/storage-of-biological-materials/desc-procedures-for-data-storage/text())" />
					<uams:textarea validation="required" instancepath="/protocol/risks/storage-of-biological-materials/desc-procedures-for-data-storage"
						id="desc-procedures-for-data-storage" value="${descProceduresForDataStorage}"/>
				</div>
				<div style="clear: both;">
					<!-- // -->
				</div>
				<div class="questionHelp">
					<uams:helpinfo
						lookupid="desc-procedures-for-data-storage" />
				</div>
			</div>
			
