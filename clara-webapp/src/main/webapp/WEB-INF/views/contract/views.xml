   <definition extends="default-ext4" name="contract/index">
		<put-attribute name="body" value="/WEB-INF/views/contract/index.jspx"/>
		<put-list-attribute name="javascripts" inherit="true">
    		<add-attribute value="/static/js/app/config.js" />
